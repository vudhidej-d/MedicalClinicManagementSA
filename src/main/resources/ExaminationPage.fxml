<GridPane alignment="center" hgap="10" prefHeight="500.0" prefWidth="650.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ExaminationController">
   <children>
      <Pane fx:id="examinationPane" prefHeight="500.0" prefWidth="650.0">
