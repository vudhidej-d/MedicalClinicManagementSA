               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Button fx:id="cancelBtn" layoutX="514.0" layoutY="443.0" mnemonicParsing="false" onAction="#cancelBtnHandle" prefHeight="30.0" prefWidth="100.0" text="CANCLE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <DatePicker fx:id="datePicker" layoutX="98.0" layoutY="118.0" prefHeight="50.0" />
