            <Label fx:id="patientLabel" layoutX="34.0" layoutY="24.0" prefHeight="50.0" prefWidth="500.0" text="*000* *Patient Name*">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <Button fx:id="submitBtn" layoutX="405.0" layoutY="443.0" mnemonicParsing="false" onAction="#submitBtnHandle" prefHeight="30.0" prefWidth="100.0" text="SUBMIT">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Label layoutX="38.0" layoutY="61.0" prefHeight="47.0" prefWidth="200.0" text="Examination">
               <font>
                  <Font name="System Italic" size="32.0" />
               </font>
            </Label>
            <Label layoutX="34.0" layoutY="123.0" prefHeight="40.0" prefWidth="50.0" text="Date:">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
            <Label layoutX="39.0" layoutY="185.0" prefHeight="40.0" prefWidth="50.0" text="Info:">
               <font>
                  <Font name="System Bold" size="19.0" />
