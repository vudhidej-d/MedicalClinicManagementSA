<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" prefHeight="500.0" prefWidth="600.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainPageController">
   <children>
      <Pane fx:id="mainPane" prefHeight="500.0" prefWidth="600.0">
         <children>
            <Button fx:id="medicalRecordsBtn" layoutX="100.0" layoutY="172.0" mnemonicParsing="false" onAction="#medicalRecordsBtnHandle" prefHeight="75.0" prefWidth="400.0" text="Medical Records">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font></Button>
            <Label alignment="CENTER" layoutY="22.0" prefHeight="134.0" prefWidth="600.0" text="Medical Clinic Management System">
               <font>
                  <Font name="System Bold Italic" size="31.0" />
               </font>
            </Label>
            <TextField fx:id="roomNumField" layoutX="419.0" layoutY="266.0" prefHeight="75.0" prefWidth="81.0" promptText="0">
               <font>
                  <Font size="30.0" />
               </font></TextField>
            <Button fx:id="dispensaryBtn" layoutX="100.0" layoutY="362.0" mnemonicParsing="false" onAction="#dispensaryBtnHandle" prefHeight="75.0" prefWidth="400.0" text="Dispensary">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font></Button>
            <Button fx:id="examinationRoomBtn" layoutX="100.0" layoutY="266.0" mnemonicParsing="false" onAction="#examinationRoomBtnHandle" prefHeight="75.0" prefWidth="300.0" text="Examination Room">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font></Button>
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
