<GridPane alignment="center" hgap="10" prefHeight="800.0" prefWidth="1000.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PatientRecordFERPController">
   <children>
      <Pane fx:id="patientRecordFERPPane" prefHeight="800.0" prefWidth="1000.0">
         <children>
            <TableView fx:id="resultTable" layoutX="50.0" layoutY="453.0" prefHeight="224.0" prefWidth="900.0">
              <columns>
                <TableColumn fx:id="resultIDColumn" prefWidth="100.0" text="Result ID" />
                <TableColumn minWidth="0.0" prefWidth="0.0" text="C2" />
                  <TableColumn fx:id="dateColumn" minWidth="0.0" prefWidth="150.0" text="Date" />
                  <TableColumn fx:id="infoColumn" minWidth="0.0" prefWidth="649.0" text="Info" />
              </columns>
            </TableView>
            <Button fx:id="backBtn" layoutX="50.0" layoutY="51.0" mnemonicParsing="false" onAction="#backBtnHandle" prefHeight="60.0" prefWidth="100.0" text="&lt;">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Button>
            <Label fx:id="patientLabel" layoutX="180.0" layoutY="31.0" prefHeight="100.0" prefWidth="750.0" text="*000* *Patient Name*">
               <font>
                  <Font name="System Bold" size="28.0" />
               </font>
            </Label>
            <Button fx:id="createBtn" layoutX="800.0" layoutY="703.0" mnemonicParsing="false" onAction="#createBtnHandle" prefHeight="60.0" prefWidth="150.0" text="CREATE">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
            </Button>
            <Label layoutX="50.0" layoutY="400.0" prefHeight="50.0" prefWidth="225.0" text="Examination Results">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
